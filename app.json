{
  "expo": {
    "name": "whatscomerceapp",
    "slug": "whatscomerceapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lvlick.whastcommerce",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.251388460791-cs38ak7mqlrlctmv1b7mvlbb0hab6ldl"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICACION",
        "USER_FACING_NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true,
      "package": "com.lvlick.whastcommerce",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}